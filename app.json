{
  "expo": {
    "name": "LottoRich",
    "slug": "LottoRich",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": ["ios","android"],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.lottorich",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544/6300978111" // sample id, replace with your own
      }
    },
    "ios": {
      "bundleIdentifier": "org.reactjs.native.example.LottoRich",
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544/6300978111" // sample id, replace with your own
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
